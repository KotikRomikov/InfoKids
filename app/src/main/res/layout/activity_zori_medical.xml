<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".zori.ZoriMedicalActivity">

    <ScrollView
        android:layout_width="match_parent"
        android:layout_height="match_parent">

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="vertical" >

            <Button
                android:id="@+id/button124"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:onClick="massage"
                android:text="массажисты" />

            <Button
                android:id="@+id/button125"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:onClick="psihologi"
                android:text="психологи" />

            <Button
                android:id="@+id/button126"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:onClick="hospital"
                android:text="Больницы" />
        </LinearLayout>
    </ScrollView>
</androidx.constraintlayout.widget.ConstraintLayout>