<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".apatiti.ApatitiMedicalActivity">

    <ScrollView
        android:layout_width="match_parent"
        android:layout_height="match_parent">

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="vertical" >

            <Button
                android:id="@+id/button167"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:onClick="doctor"
                android:text="Врачи" />

            <Button
                android:id="@+id/button168"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:onClick="massage"
                android:text="массажисты" />

            <Button
                android:id="@+id/button169"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:onClick="logoped"
                android:text="логопед" />

            <Button
                android:id="@+id/button170"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:onClick="psihologi"
                android:text="психолог" />

            <Button
                android:id="@+id/button171"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:onClick="sanative"
                android:text="Оздоровительные" />

            <Button
                android:id="@+id/button172"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:onClick="privateHospital"
                android:text="Частные клиники" />

            <Button
                android:id="@+id/button173"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:onClick="hospital"
                android:text="Больницы" />
        </LinearLayout>
    </ScrollView>
</androidx.constraintlayout.widget.ConstraintLayout>