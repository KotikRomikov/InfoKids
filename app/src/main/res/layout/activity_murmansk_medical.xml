<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".murmansk.MurmanskMedicalActivity">

    <ScrollView
        android:layout_width="match_parent"
        android:layout_height="match_parent">

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="vertical" >

            <Button
                android:id="@+id/pediatorMurmansk"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:text="@string/pediator" />

            <Button
                android:id="@+id/nevrologMurmansk"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:text="@string/nevrolog" />

            <Button
                android:id="@+id/massagMurmansk"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:text="@string/massage" />

            <Button
                android:id="@+id/logopediMurmansk"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:text="@string/logoped" />

            <Button
                android:id="@+id/psihologiMurmansk"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:text="@string/psihologi" />

            <Button
                android:id="@+id/grudnMurmansk"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:text="@string/grudnoe" />

            <Button
                android:id="@+id/hospitalMurmansk"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:text="@string/hospital" />

            <Button
                android:id="@+id/rodiHouseMurmansk"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:text="@string/rodi_house" />

            <Button
                android:id="@+id/privateHospitalMurmansk"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:text="@string/private_hospital" />
        </LinearLayout>
    </ScrollView>

</androidx.constraintlayout.widget.ConstraintLayout>